#ifndef _JAVA_AWT_RECTANGLE_
#define _JAVA_AWT_RECTANGLE_

#include "../../cpp2javalib.h"

/*------------------------------------------------------------*/

class Rectangle : public Object//Rectangle2D
    //, Cloneable, Serializable, Shape
{
public:
    Rectangle();
    Rectangle(int x, int y, int width, int height);
    virtual ~Rectangle();
    virtual double getWidth();
    virtual double getHeight();
    int x, y, width, height;
};

