#ifndef _JAVA_AWT_IMAGE_COLORMODEL_
#define _JAVA_AWT_IMAGE_COLORMODEL_

#include "../../../cpp2javalib.h"

/*------------------------------------------------------------*/

class ColorModel : public Object
{
public:
    ColorModel(int bits);
    virtual ~ColorModel();
    virtual int getPixelSize();
    virtual int getRGB(int pixel);
protected:
    int bits;
};

/*------------------------------------------------------------*/

#endif /* #ifndef _JAVA_AWT_IMAGE_COLORMODEL_ */
