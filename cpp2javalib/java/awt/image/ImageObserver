#ifndef _JAVA_AWT_IMAGE_IMAGEOBSERVER_
#define _JAVA_AWT_IMAGE_IMAGEOBSERVER_

#include "../../../cpp2javalib.h"

/*------------------------------------------------------------*/

class ImageObserver
{
public:
    enum
    {
        WIDTH       = 0x01,
        HEIGHT      = 0x02,
        PROPERTIES  = 0x04,
        SOMEBITS    = 0x08,
        FRAMEBITS   = 0x10,
        ALLBITS     = 0x20,
#undef ERROR
        ERROR       = 0x40,
        ABORT       = 0x80,
    };
};

/*------------------------------------------------------------*/

#endif /* #ifndef _JAVA_AWT_IMAGE_IMAGEOBSERVER_ */
