#ifndef _JAVA_AWT_EVENT_MOUSELISTENER_
#define _JAVA_AWT_EVENT_MOUSELISTENER_

#include "../../../cpp2javalib.h"

/*------------------------------------------------------------*/

class MouseListener
{
public:
    virtual void mousePressed(MouseEvent* e) = 0;
    virtual void mouseReleased(MouseEvent* e) = 0;
};

/*------------------------------------------------------------*/

#endif /* #ifndef _JAVA_AWT_EVENT_MOUSELISTENER_ */
