#ifndef _JAVA_AWT_EVENT_KEYLISTENER_
#define _JAVA_AWT_EVENT_KEYLISTENER_

#include "../../../cpp2javalib.h"

class KeyEvent;

/*------------------------------------------------------------*/

class KeyListener
{
public:
    virtual void keyTyped(KeyEvent* e) = 0;
    virtual void keyPressed(KeyEvent* e) = 0;
    virtual void keyReleased(KeyEvent* e) = 0;
};

/*------------------------------------------------------------*/

#endif /* #ifndef _JAVA_AWT_EVENT_KEYLISTENER_ */
