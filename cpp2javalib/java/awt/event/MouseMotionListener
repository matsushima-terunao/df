#ifndef _JAVA_AWT_EVENT_MOUSEMOTIONLISTENER_
#define _JAVA_AWT_EVENT_MOUSEMOTIONLISTENER_

#include "../../../cpp2javalib.h"

class MouseEvent;

/*------------------------------------------------------------*/

class MouseMotionListener
{
public:
    virtual void mouseDragged(MouseEvent* e) = 0;
    virtual void mouseMoved(MouseEvent* e) = 0;
};

/*------------------------------------------------------------*/

#endif /* #ifndef _JAVA_AWT_EVENT_MOUSEMOTIONLISTENER_ */
